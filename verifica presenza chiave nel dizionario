def count_enemies(enemy_names):
    enemies_dict = {}
    for enemy_name in enemy_names:
        if enemy_name in enemies_dict: 
          enemies_dict[enemy_name] += 1 # se la chiave è già presente nel dizionario aumento di uno il suo valore
        else:
          enemies_dict[enemy_name] = 1 # altrimenti creo la chiave e inizio il conteggio da 1
       
    return enemies_dict
